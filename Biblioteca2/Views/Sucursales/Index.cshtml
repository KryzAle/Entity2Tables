@model IEnumerable<Biblioteca2.Sucursal>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.suc_nom)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.suc_direc)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.suc_nom)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.suc_direc)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.suc_id }) |
            @Html.ActionLink("Details", "Details", new { id=item.suc_id }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.suc_id })
        </td>
    </tr>
}

</table>
